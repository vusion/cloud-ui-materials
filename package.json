{
  "name": "cloud-ui-materials",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "scripts": {
    "changelog": "node scripts/changelog.js",
    "changelog:ci": "node scripts/changelog.js from-ci",
    "changelog:comment": "node scripts/changelog.js from-comment",
    "build": "turbo run build",
    "build:fast": "turbo run build --parallel",
    "build:ci": "turbo run build --filter='./workspaces/ts-vue2...' --filter='./workspaces/legacy-yaml...' --filter='!./workspaces/legacy-yaml/packages/cw/cw_oa_sdk' --filter='!./workspaces/legacy-yaml/packages/cw/cw_office_sdk' --filter='!./workspaces/legacy-yaml/packages/cw/cw_swiper_library' --filter='!./workspaces/legacy-yaml/packages/cw/popo_sdk' --filter='!./workspaces/legacy-yaml/packages/lcap/lcap_popo_location'",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck"
  },
  "dependencies": {
    "dom-to-image": "^2.6.0",
    "install": "^0.13.0",
    "lodash.mergewith": "^4.6.2",
    "node-fetch": "2",
    "npm": "^11.0.0",
    "vue-tour": "^2.0.0"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "turbo": "2.5.8",
    "terser": "^5.44.0",
    "vue": "2.7.16",
    "vue-template-compiler": "2.7.16",
    "@vue/compiler-sfc": "2.7.16",
    "css-loader": "^7.1.2",
    "fs-extra": "^11.3.2",
    "prop-types": "^15.8.1"
  },
  "engines": {
    "node": ">=18 <=20"
  },
  "pnpm": {
    "overrides": {
      "@popo-bridge/web": "link:./workspaces/legacy-yaml/stubs/popo-bridge-web",
      "@oa/iframe-channel": "link:./workspaces/legacy-yaml/stubs/oa/iframe-channel",
      "@popo-bridge/web-vue2": "link:./workspaces/legacy-yaml/stubs/popo-bridge-web-vue2",
      "vue-loader": "^15.10.1",
      "vue-template-compiler": "2.7.16",
      "webpack": "^4.47.0",
      "babel-loader": "^8.3.0",
      "postcss-loader": "^3.0.0",
      "less-loader": "^7.3.0",
      "html-webpack-plugin": "^3.2.0",
      "less": "4.1.3",
      "lodash.get": "^4.4.2",
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "postcss": "^8.4.38",
      "vue": "2.7.16",
      "@vue/compiler-sfc": "2.7.16"
    }
  }
}
