{
  "name": "cust-adl",
  "sourceRoot": "packages/cust/adl/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm -C packages/cust/adl run build"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm -C packages/cust/adl run lint || echo 'no lint script'"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm -C packages/cust/adl run test || echo 'no test script'"
      }
    }
  },
  "tags": [
    "stack:react"
  ]
}
