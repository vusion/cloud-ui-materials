                "name": "width",
                "type": "string",
                "default": 800,
                "description": "宽度"
            },
            {
                "name": "height",
                "type": "string",
                "default": 600,
                "description": "高度"
            },
            {
                "name": "value",
                "type": "Array",
                "model": true,
                "sync": true,
                "default": "[]",
                "description": "当前文件列表"
            },
            {
                "name": "url",
                "type": "string",
                "required": true,
                "default": null,
                "description": "上传的地址"
            },
            {
                "name": "converter",
                "title": "转换器",
                "type": "string",
                "options": [
